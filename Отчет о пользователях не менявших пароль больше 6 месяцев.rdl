  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AD_source</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Disabled">
            <Value>=Parameters!Disabled.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
displayName
, DATEADD(minute, DATEDIFF(minute, GETUTCDATE(), GETDATE()), CASE WHEN CAST([pwdLastSet] AS bigint) &gt; 0 THEN CAST([pwdLastSet] AS bigint) / (864000000000.0) ELSE 109207 END)
		 - 109207 AS pwdLastSet
, DATEADD(minute, DATEDIFF(minute, GETUTCDATE(), GETDATE()), CASE WHEN CAST([badPasswordTime] AS bigint) &gt; 0 THEN CAST([badPasswordTime] AS bigint) / (864000000000.0) ELSE 109207 END)
		 - 109207 AS badPasswordTime
, badPwdCount
, userAccountControl &amp; 2 AS Disabled
, sAMAccountName
, telephoneNumber
, mobile
, department
, distinguishedName
, SUBSTRING(distinguishedName, 
        CHARINDEX('DC=', distinguishedName)+3,
        CHARINDEX('DC=', SUBSTRING(distinguishedName,CHARINDEX('DC=', distinguishedName)+5,LEN(distinguishedName)))
 ) AS Domain
FROM OpenQuery
(
ADSI,
'SELECT pwdLastSet, badPasswordTime, badPwdCount, userAccountControl, pager, displayName, telephoneNumber, sAMAccountName,
mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname, distinguishedName
FROM  ''LDAP://domain.corp''
WHERE objectClass =  ''User'' AND objectCategory = ''Person'' 
') AS tblADSI where DATEADD(minute, DATEDIFF(minute, GETUTCDATE(), GETDATE()), CASE WHEN CAST([pwdLastSet] AS bigint) &gt; 0 THEN CAST([pwdLastSet] AS bigint) / (864000000000.0) ELSE 109207 END) - 109207 BETWEEN '1900-01-01 00:00:00.000' AND  GETDATE()-180 AND userAccountControl &amp; 2 IN (@Disabled)
UNION
SELECT 
displayName
, DATEADD(minute, DATEDIFF(minute, GETUTCDATE(), GETDATE()), CASE WHEN CAST([pwdLastSet] AS bigint) &gt; 0 THEN CAST([pwdLastSet] AS bigint) / (864000000000.0) ELSE 109207 END)
		 - 109207 AS pwdLastSet
, DATEADD(minute, DATEDIFF(minute, GETUTCDATE(), GETDATE()), CASE WHEN CAST([badPasswordTime] AS bigint) &gt; 0 THEN CAST([badPasswordTime] AS bigint) / (864000000000.0) ELSE 109207 END)
		 - 109207 AS badPasswordTime
, badPwdCount
, userAccountControl &amp; 2 AS Disabled
, sAMAccountName
, telephoneNumber
, mobile
, department
, distinguishedName
, SUBSTRING(distinguishedName, 
        CHARINDEX('DC=', distinguishedName)+3,
        CHARINDEX('DC=', SUBSTRING(distinguishedName,CHARINDEX('DC=', distinguishedName)+5,LEN(distinguishedName)))
 ) AS Domain
FROM OpenQuery
(
ADSI,
'SELECT pwdLastSet, badPasswordTime, badPwdCount, userAccountControl, pager, displayName, telephoneNumber, sAMAccountName,
mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname, distinguishedName
FROM  ''LDAP://subdomain1.domain.corp''
WHERE objectClass =  ''User'' AND objectCategory = ''Person'' 
') AS tblADSI where DATEADD(minute, DATEDIFF(minute, GETUTCDATE(), GETDATE()), CASE WHEN CAST([pwdLastSet] AS bigint) &gt; 0 THEN CAST([pwdLastSet] AS bigint) / (864000000000.0) ELSE 109207 END) - 109207 BETWEEN '1900-01-01 00:00:00.000' AND  GETDATE()-180 AND userAccountControl &amp; 2 IN (@Disabled)
UNION
SELECT 
displayName
, DATEADD(minute, DATEDIFF(minute, GETUTCDATE(), GETDATE()), CASE WHEN CAST([pwdLastSet] AS bigint) &gt; 0 THEN CAST([pwdLastSet] AS bigint) / (864000000000.0) ELSE 109207 END)
		 - 109207 AS pwdLastSet
, DATEADD(minute, DATEDIFF(minute, GETUTCDATE(), GETDATE()), CASE WHEN CAST([badPasswordTime] AS bigint) &gt; 0 THEN CAST([badPasswordTime] AS bigint) / (864000000000.0) ELSE 109207 END)
		 - 109207 AS badPasswordTime
, badPwdCount
, userAccountControl &amp; 2 AS Disabled
, sAMAccountName
, telephoneNumber
, mobile
